<% _.each(collection, function(collection) { %>
		<li draggable="true" data-id="<%= collection.id %>"  style="line-height:16px" class="col-lg-3 facebook-contact k-font">
			<div id="facebook-contact-box">
				<p class="col-lg-12" style="margin-bottom:5px"><img src="https://graph.facebook.com/<%= collection.id %>/picture" class="facebook-contact-photo unselectable" draggable="false"/></p> 
				<p class="facebook-contact-name col-lg-12"><%= collection.name %></p> 
			</div>
		</li>
<% }); %>