<% _.each(collection, function(collection) { %>
		<li draggable="true" data-id="<%= collection.id %>" style="line-height:16px" class="col-lg-3 contact-outer k-font">
			<div class="contact-inner">
				<p class="col-lg-12" style="margin-bottom:5px"><img src="https://graph.facebook.com/<%= collection.id %>/picture" class="contact-photo unselectable" draggable="false"/></p> 
				<p class="contact-name col-lg-12"><%= collection.name %></p> 
			</div>
		</li>
<% }); %>