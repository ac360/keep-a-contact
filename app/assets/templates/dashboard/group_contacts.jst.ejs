<div class="row">
	<div class="col-lg-12">
		<h5 class="k-centered k-grey-one" style="padding-top:5px;"><%= group %></h5>
	</div>
</div>
<div class="row">
	<div id="" class="col-lg-12 scrollable-container">
		<ul id="contacts-list" class="list-inline row">
			<% _.each(collection, function(collection) { %>
					<li draggable="true" data-id="<%= collection.id %>"  style="line-height:16px" class="col-lg-4 contact-outer k-font animated">
						<div class="contact-inner">
							<p class="col-lg-12" style="margin-bottom:5px"><img src="<%= collection.image_url %>" class="contact-photo unselectable" draggable="false"/></p> 
							<p class="contact-name col-lg-12"><%= collection.full_name %></p> 
							<p class="contact-group col-lg-12"><% if(group != "All"){%><%= group %><% } %></p> 
							<p class="contact-priority col-lg-12">
								<% if (collection.priority == 1) { %> <span class="priority-option c-priority">1</span>  
								<% } else { %> <span class="priority-option">1</span>  <% } %>
								<% if (collection.priority == 2) { %> <span class="priority-option c-priority">2</span>  
								<% } else { %> <span class="priority-option">2</span>  <% } %>
								<% if (collection.priority == 3) { %> <span class="priority-option c-priority">3</span>  
								<% } else { %> <span class="priority-option">3</span>  <% } %>
								<% if (collection.priority == 4) { %> <span class="priority-option c-priority">4</span>  
								<% } else { %> <span class="priority-option">4</span>  <% } %>
								<% if (collection.priority == 5) { %> <span class="priority-option c-priority">5</span>  
								<% } else { %> <span class="priority-option">5</span>  <% } %>
							</p>
						</div>
					</li>
			<% });  %>
		<ul>
	</div>
</div>

